{
   "type" : "fedora-river",
   "jms" : {
      "topicFilter" : "fedora.apim.update",
      "brokerUrl" : "failover:(tcp://localhost:61616)",
      "messageSelector" : "methodName LIKE 'modify%' OR methodName LIKE 'add%' OR methodName LIKE 'purge%' OR methodName = 'ingest'"
   },
   "fedora" : {
      "url" : "http://localhost:8080/fedora",
      "password" : "fedoraAdmin",
      "username" : "fedoraAdmin"
   },
   "index" : {
      "exclude_datastreams" : [
         "DC",
         "RELS-EXT",
         "RELS-INT",
         "POLICY",
         "QUCOSA-XML",
         "MODS",
         "SLUB-INFO"
      ],
      "dissemination" : {
         "relevant_datastreams" : [
            "MODS",
            "SLUB-INFO"
         ],
         "properties" : {
            "PUB_TYPE" : {
               "store" : true,
               "index" : "not_analyzed",
               "type" : "string"
            },
            "PUB_URN" : {
               "index" : "not_analyzed",
               "store" : true,
               "type" : "string"
            }
         },
         "sdef_pid" : "qucosa:SDef",
         "method" : "getIndexDocument"
      },
      "pid_match" : "^qucosa:\\d+$",
      "name" : "fedora"
   },
   "oai" : {
      "url" : "http://localhost:8080/fedora/oai",
      "poll_interval" : "5m"
   }
}
